  "type": "module",
  "scripts": {
    "prepare": "husky",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint . --no-warn-ignored",
